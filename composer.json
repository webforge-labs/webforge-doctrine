{
    "name": "webforge/doctrine",
    "description": "Doctrine ORM bridge for webforge",
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Scheit",
            "email": "p.scheit@ps-webforge.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Webforge\\Doctrine": ["lib/", "tests/"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=5.3.2",
      "doctrine/orm": ">=2.2.3,<2.6"
    },
    "suggests": {
        "doctrine/data-fixtures": "to use the database testcase",
        "webforge/console": "to use the commands for managing doctrine"
    },
    "require-dev": {
        "webforge/webforge": "dev-master",
        "webforge/testdata-repository": "*@alpha",
        "webforge/testplate": ">=1.2@dev",
        "mockery/mockery": "dev-master",
        "liip/rmt": "0.9.*",
        "satooshi/php-coveralls": "dev-master",
        "doctrine/data-fixtures": "1.0.*@dev",
        "jms/serializer": "0.15.*@stable"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        },

        "directory-locations": {
            "doctrine-entities": "lib/Webforge/Doctrine/Test/Entities/"
        }
    }
}
